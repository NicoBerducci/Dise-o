<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
 
<hibernate-mapping> 
 
    <class name="entidades.TramiteEstado" table="tramiteestado"> 
 
        <id name="OID" type="string"> 
            <column name="OIDTramiteEstado"/> 
            <generator class="uuid2"/> 
        </id> 
 
          <property name="contadorEstado" type="int"> 
            <column name="contadorEstado"/> 
        </property> 
 
        <property name="etapa" type="int"> 
            <column name="etapa"/> 
        </property> 
        
        <property name="fechaBajaTramiteEstado" type="date"> 
            <column name="fechaBajaTramiteEstado"/> 
        </property> 
        
        <property name="fechaFinTramiteEstado" type="date"> 
            <column name="fechaFinTramiteEstado"/> 
        </property> 
        
        <property name="fechaInicioTramiteEstado" type="date"> 
            <column name="fechaInicioTramiteEstado"/> 
        </property> 
        
        <many-to-one name="estadoTramite" class="entidades.EstadoTramite" column="OIDEstadoTramite" not-null="true"/>
        
    </class> 
 
</hibernate-mapping>
