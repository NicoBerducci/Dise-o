<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.EstadoTramite" table="estadotramite">

        <id name="OID" type="string">
            <column name="OIDEstadoTramite"/>
            <generator class="uuid2"/>
        </id>

          <property name="codEstadoTramite" type="int">
            <column name="codEstadoTramite"/>
        </property>

        <property name="nombreEstadoTramite" type="string">
            <column name="nombreEstadoTramite"/>
        </property>
        
        <property name="descripcionEstadoTramite" type="string">
            <column name="descripcionEstadoTramite"/>
        </property>
        
        <property name="fechaBajaEstadoTramite" column="fecha_baja" type="timestamp"/>
    </class>

</hibernate-mapping>