<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.TipoTramiteEstadoTramite" table="tipotramiteestadotramite">

        <id name="OID" type="string">
            <column name="OIDTipoTramiteEstadoTramite"/>
            <generator class="uuid2"/>
        </id>

          <property name="contadorEtapaEstado" type="int">
            <column name="contadorEtapaEstado"/>
        </property>
        
        <many-to-one name="estadoTramiteOrigen" class="entidades.EstadoTramite" column="OIDEstadoTramite" not-null="true"/>
        
        <bag name="estadoTramiteDestinoList" table="estadotramitedestinos" cascade="none">
        <key column="OIDTipoTramiteEstadoTramite" not-null="true"/>
        <many-to-many class="entidades.EstadoTramite" column="OIDEstadoTramite"/>
        </bag>
 
    </class>

</hibernate-mapping>