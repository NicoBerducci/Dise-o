<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.Tramite" table="tramite">

        <id name="OID" type="string">
            <column name="OIDTramite"/>
            <generator class="uuid2"/>
        </id>

          <property name="fechaAnulacion" type="date">
            <column name="fechaAnulacion"/>
        </property>

        <property name="fechaHoraFinTramiteReal" type="timestamp">
            <column name="fechaHoraFinTramiteReal"/>
        </property>
        
        <property name="fechaHoraInicioTramiteReal" type="timestamp">
            <column name="fechaHoraInicioTramiteReal"/>
        </property>
        
        <property name="fechaHoraLimiteDeEntregaDoc" type="timestamp">
            <column name="fechaHoraLimiteDeEntregaDoc"/>
        </property>

        <property name="fechaHoraRegistroTramite" type="timestamp">
            <column name="fechaHoraRegistroTramite"/>
        </property>
        
        <property name="nroTramite" type="int">
            <column name="nroTramite"/>
        </property>
        
        <property name="Observaciones" type="string">
            <column name="Observaciones"/>
        </property>
        
        <property name="TramiteCosto" type="int">
            <column name="TramiteCosto"/>
        </property>
        
        <many-to-one name="consultor" class="entidades.Consultor" column="OIDConsultor" not-null="true"/>
        
        <many-to-one name="cliente" class="entidades.Cliente" column="OIDCliente" not-null="true"/>
        
        <many-to-one name="estadoTramite" class="entidades.EstadoTramite" column="OIDEstadoTramite" not-null="true"/>
        
        
        <bag name="tramiteEstadoList" table="tramiteestado" cascade="save-update" inverse="false">
        <key column="OIDTramite" not-null="false"/>
        <one-to-many class="entidades.TramiteEstado"/>
        </bag>
        
        <bag name="tramiteDocumentacionList" table="tramitedocumentacion" cascade="save-update" inverse="false">
        <key column="OIDTramite" not-null="true"/>
        <one-to-many class="entidades.TramiteDocumentacion"/>
        </bag>
        
        
        <many-to-one name="tipoTramite" class="entidades.TipoTramite" column="OIDTipoTramite" not-null="true"/>
   
    </class>

</hibernate-mapping>